{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "space-before-function-paren": [
      "error",
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always"
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
    // "import/resolver": {
    //   "webpack": {
    //     "config": "./internals/webpack/webpack.prod.babel.js"
    //     // "config": "./"
    //   },
    //   "node": {
    //     "extensions": [".js", ".jsx", ".ts", ".tsx"]
    //   }
    // }
  }
}
